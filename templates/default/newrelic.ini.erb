; WARNING: This file is managed via Opscode Chef. Any manual changes will be overridden on chef run.

newrelic.appname                                  = <%= @appname %>
newrelic.enabled                                  = <%= @params[:newrelic_agent][:php]['newrelic.enabled'] %>
newrelic.transaction_tracer.enable                = <%= @params[:newrelic_agent][:php]['newrelic.transaction_tracer.enable']  %>
newrelic.transaction_tracer.threshold             = <%= @params[:newrelic_agent][:php]['newrelic.transaction_tracer.threshold'] %>
newrelic.transaction_tracer.top100']              = <%= @params[:newrelic_agent][:php]['newrelic.transaction_tracer.top100'] %>
newrelic.transaction_tracer.record_sql            = <%= @params[:newrelic_agent][:php]['newrelic.transaction_tracer.record_sql'] %>
newrelic.transaction_tracer.stack_trace_theshold  = <%= @params[:newrelic_agent][:php]['newrelic.transaction_tracer.stack_trace_theshold'] %>
newrelic.transaction_tracer.custom                = <%= @params[:newrelic_agent][:php]['newrelic.transaction_tracer.custom'] %>
newrelic.logfile                                  = <%= @params[:newrelic_agent][:php]['newrelic.logfile'] %>
newrelic.loglevel                                 = <%= @params[:newrelic_agent][:php]['newrelic.loglevel'] %>
newrelic.enable_params                            = <%= @params[:newrelic_agent][:php]['newrelic.enable_params'] %>
newrelic.error_collector.enable                   = <%= @params[:newrelic_agent][:php]['newrelic.error_collector.enable'] %>
newrelic.error_collector.record_database_errors   = <%= @params[:newrelic_agent][:php]['newrelic.error_collector.record_database_errors'] %>
newrelic.webtransaction.name.function             = <%= @params[:newrelic_agent][:php]['newrelic.webtransaction.name.function'] %>
newrelic.webtransaction.name.files                = <%= @params[:newrelic_agent][:php]['newrelic.webtransaction.name.files'] %>
newrelic.webtransaction.name.remove_trailing_path = <%= @params[:newrelic_agent][:php]['newrelic.webtransaction.name.remove_trailing_path'] %>
newrelic.framework                                = <%= @params[:newrelic_agent][:php]['newrelic.framework'] %>
newrelic.browser_monitoring.auto_instrument       = <%= @params[:newrelic_agent][:php]['newrelic.browser_monitoring.auto_instrument'] %>
newrelic.timeout                                  = <%= @params[:newrelic_agent][:php]['newrelic.timeout'] %>
newrelic.nonblocking                              = <%= @params[:newrelic_agent][:php]['newrelic.nonblocking'] %>
newrelic.daemon                                   = <%= @params[:newrelic_agent][:php]['newrelic.daemon']   %>

;Add the New Relic Extension to PHP5
extension=newrelic.so
